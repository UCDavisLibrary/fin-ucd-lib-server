    "archiver": "^3.1.1",
    "body-parser": "^1.18.2",
    "clone": "^2.1.1",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "elasticsearch": "^16.3.0",
    "express": "^4.16.2",
    "request": "^2.87.0",
    "tar-stream": "^1.6.1"
  },
  "devDependencies": {}
}
