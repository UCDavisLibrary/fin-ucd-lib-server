{
  "name": "ucd-lib-client",
  "version": "0.2.1",
  "description": "UI client application for UCD Library DAMS",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd client && webpack",
    "watch": "cd client && webpack --watch",
